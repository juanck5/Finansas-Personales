

generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Movimiento{
  id  Int @id @default(autoincrement())
  descripcion String
  monto Float
  tipo String
  fecha DateTime @default(now())

}